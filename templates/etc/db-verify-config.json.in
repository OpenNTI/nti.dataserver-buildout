                }
            ]
        },
        "okstate": {
            "servers": [
                {
                    "filename": "Users.sql",
                    "socket": "/var/run/mysqld/9-1.sock"
                },
                {
                    "filename": "Analytics-Tahrir.sql",
                    "socket": "/var/run/mysqld/9-2.sock"
                }
            ]
        },
        "outest": {
            "servers": [
                {
                    "filename": "Users-Users_1_Test-Users_2_Test-Tahrir-Analytics.sql",
