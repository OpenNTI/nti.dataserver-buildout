<!-- -*- mode: nxml -*- -->
<configure	xmlns="http://namespaces.zope.org/zope"
			xmlns:i18n="http://namespaces.zope.org/i18n"
			xmlns:zcml="http://namespaces.zope.org/zcml"
			xmlns:sooner="http://nextthought.com/ntp/evergage">

	<include package="nti.ou.evergage" file="meta.zcml" />
	<sooner:registerSalesforceAPI
			sandbox="${evergage:sandbox}"
			username="${evergage:username}"
			password="${evergage:password}"
			security_token="${evergage:security_token}" />

</configure>
