	<sp:grant principal="jae.strickland" role="nti.roles.contentlibrary.admin"/>

	<sp:principal id="janelle.barrick" login="janelle.barrick" title="Janelle Barrick"/>
	<sp:grant principal="janelle.barrick" role="nti.roles.contentlibrary.admin"/>

	<sp:principal id="lacey.white" login="lacey.white" title="Lacey White"/>
	<sp:grant principal="lacey.white" role="nti.roles.contentlibrary.admin"/>

	<sp:principal id="ashley.stewart" login="ashley.stewart" title="Ashley Stewart"/>
