<!-- -*- mode: nxml -*- -->
<configure	xmlns="http://namespaces.zope.org/zope"
			xmlns:i18n="http://namespaces.zope.org/i18n"
			xmlns:zcml="http://namespaces.zope.org/zcml"
			xmlns:starrez="http://nextthought.com/ntp/starrez">

	<include package="nti.oubound.housing.starrez" file="meta.zcml" />

	<include package="z3c.baseregistry" file="meta.zcml" />

	<registerIn registry="nti.app.oubound.housing.sites.HOUSING">
			<starrez:registerStarrezDB
					 dburi="${starrez:protocol}://${starrez:user}:${starrez:password}@${starrez:host}/${starrez:dbname}?driver=${starrez:driver}"
					 twophase="False"
					 autocommit="False"
					 security_user_id="${starrez:security_user_id}"
 					 timezone="${starrez:timezone}"/>
	</registerIn>

</configure>
